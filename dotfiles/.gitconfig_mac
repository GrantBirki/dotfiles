# This key is specific to my MacBook Air

[core]
	autocrlf = false
[user]
	email = grant.birkinbine@gmail.com
	name = GrantBirki
	signingkey = 9C0C622D9DEC4A08
[gpg]
	program = /opt/homebrew/bin/gpg
[url "git@github.com:"]
	insteadOf = https://github.com/
[commit]
	gpgSign = true
[pull]
	rebase = false
[sequence]
	editor = code --wait
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
