[core]
	autocrlf = false
[user]
	email = grant.birkinbine@gmail.com
	name = GrantBirki
	signingkey = 65497A530F6F9405
[gpg]
	program = gpg
[url "git@github.com:"]
	insteadOf = https://github.com/
[commit]
	gpgSign = true
[pull]
	rebase = false
[sequence]
	editor = code --wait
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
